{
    "name": "ComposedFeed",
    "base": "PersistedModel",
    "strict": true,
    "idInjection": true,
    "http": {"path": "/feeds/composed"},
    "properties": {
        "storage": { "type": "boolean", "default": false },
        "readable": { "type": "boolean", "default": false },
        "writeable": { "type": "boolean", "default": false }
    },
    "validations": [],
    "relations": {
        "fields": {
            "type": "embedsMany",
            "model": "Field",
            "property": "fields",
            "options": {
                "persistent": true,
                "forceId": true
            }
        }
    },
    "acls": [
        {
            "accessType": "*",
            "property": "*",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        },
        {
            "accessType": "READ",
            "property": "*",
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW"
        },
        {
            "accessType": "*",
            "property": "*",
            "principalType": "ROLE",
            "principalId": "admin",
            "permission": "ALLOW"
        }
    ],
    "mixins": {
        "BaseFeed": {"type": "composed"}
    },
    "methods": []
}
