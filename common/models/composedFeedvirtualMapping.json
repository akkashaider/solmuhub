{
    "name": "ComposedFeedVirtualMapping",
    "base": "PersistedModel",
    "strict": true,
    "idInjection": true,
    "http": {
        "path": "/feeds/composedVirtualMapping"
    },
    "properties": {
        "name": {
            "type": "string",
            "required": true
        },
        "fields": {
            "type": ["string"],
            "default": []
        }
    },
    "relations": {
        "feed": {
            "type": "belongsTo",
            "model": "ComposedFeed",
            "foreignKey": "feedId"
        }
    },
    "acls": [
        {
            "accessType": "*",
            "property": "*",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        },
        {
            "accessType": "*",
            "property": "*",
            "principalType": "ROLE",
            "principalId": "admin",
            "permission": "ALLOW"
        }
    ]
}