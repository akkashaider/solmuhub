{
    "initial:before": {
        "loopback#favicon": {}
    },
    "initial": {},
    "session": {},
    "auth": {},
    "parse": {
        "body-parser#json": {"params":{"limit":"50mb"}},
        "body-parser#raw": {"params":{"limit":"50mb"}},
        "body-parser#text": {
            "params":{"limit":"50mb"},
            "methods":["post"],
            "paths":["/api/feeds/executable"]
        }
    },
    "routes": {
        "loopback#rest": {
            "paths": [
                "${restApiRoot}"
            ]
        }
    },
    "files": {
        "loopback#static": {
            "params": "$!../client"
        }
    },
    "final": {
        "loopback#urlNotFound": {}
    },
    "final:after": {
        "loopback#errorHandler": {}
    }
}
